{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Infrastructure": {
    "NetworkPerspectiveCore": {
      "BaseUrl": "",
      "MaxInteractionsPerRequestCount": 20000,
      "Resiliency": {
        "Retries": [
          "00:00:02.000",
          "00:00:05.000",
          "00:00:10.000",
          "00:01:00.000"
        ]
      }
    }
  },
  "ConnectionStrings": {
    "Database": ""
  },
  "App": {
    "Scheduler": {
      "CronExpression": "0 0 0 * * ?",
      "UsePersistentStore": true
    }
  }
}
